{
	"name": "Random Passwords Generator",
	"description": "Generates Strong Randomized Passwords !",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"storage"
	],
	"action": {
		"default_popup": "popup/popup.html"
	},
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png",
		"256": "icons/256.png",
		"512": "icons/512.png"
	}
}